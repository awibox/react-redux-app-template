{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["__REDUX_DEVTOOLS_EXTENSION__"]
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
