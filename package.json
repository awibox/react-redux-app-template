{
  "name": "react-redux-app-template",
  "version": "1.0.0",
  "description": "The template that will help you quickly start developing your project using React.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awibox/react-redux-app-template.git"
  },
  "keywords": [
    "react",
    "redux-thunk",
    "css-modules",
    "webpack",
    "boilerplate",
    "react-router",
    "yarn",
    "react16",
    "scss",
    "prop-types"
  ],
  "author": "awibox",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-immutable-proptypes": "^2.2.0",
    "react-promise-loader": "^1.0.5",
    "react-promise-tracker": "^2.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "tigerspack": "^0.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.10.3",
    "autoprefixer": "^9.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.4.0",
    "jest-css-modules-transform": "^4.0.0",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "start": "webpack-dev-server --open",
    "lint": "eslint --report-unused-disable-directives src",
    "fix": "eslint --fix --report-unused-disable-directives src",
    "setup": "cp ./.github/hooks/pre-commit ./.git/hooks/pre-commit && chmod +x ./.git/hooks/pre-commit",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/*.{js,jsx,mjs}",
      "!src/actions/**/*.{js,jsx,mjs}",
      "!src/data/**/*.{js,jsx,mjs}",
      "!src/selectors/**/*.{js,jsx,mjs}",
      "!src/containers/**/*.{js,jsx,mjs}",
      "!src/pages/**/*.{js,jsx,mjs}",
      "!src/reducers/index.js"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "snapshotResolver": "<rootDir>/spec/jestSnapshotResolver",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/spec/jestSetupTests.js",
      "<rootDir>/spec/jestSetupJsDom.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    }
  },
  "bugs": {
    "url": "https://github.com/awibox/react-redux-app-template/issues"
  },
  "homepage": "https://github.com/awibox/react-redux-app-template#readme",
  "main": "webpack.config.js"
}
